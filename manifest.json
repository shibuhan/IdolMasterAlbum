{
  "name": "IdolMasterAlbum",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "IdolMasterAlbum",
  "content_scripts": [{
    "matches": ["https://columbia.jp/idolmaster/*","http://www.lantis.jp/imas/*","https://www.lantis.jp/imas/*"],
    "js": [
      "jquery-3.5.0.min.js",
      "content.js"
    ]
  }],
  "permissions" : [
		"contextMenus"
  ],
  "background" : {
    "background.service_worker" : [
      "jquery-3.5.0.min.js",
      "background.js"]
  }
}